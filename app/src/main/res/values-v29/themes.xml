<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.CopyPasta" parent="Theme.MaterialComponents.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#009c74</item>
        <item name="colorPrimaryVariant">#007f58</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#8b43a9</item>
        <item name="colorSecondaryVariant">#6d3898</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="fontFamily">@font/font</item>
        <item name="materialButtonStyle">@style/Theme.CopyPasta.Button</item>

    </style>
</resources>